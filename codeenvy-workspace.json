{
  "projects": [
    {
      "mixins": [
        "pullrequest"
      ],
      "problems": [],
      "source": {
        "location": "https://github.com/lyuboraykov/codenvy-spark-java",
        "type": "git",
        "parameters": {}
      },
      "name": "project-web",
      "type": "maven",
      "path": "/project-web",
      "attributes": {
        "contribute_to_branch": [
          "master"
        ]
      }
    }
  ],
  "commands": [
    {
      "commandLine": "mysql -u $MYSQL_USER -p$MYSQL_PASSWORD -e 'show databases;'",
      "name": "show databases",
      "type": "custom",
      "attributes": {}
    },
    {
      "commandLine": "clean install -DskipTests",
      "name": "codenvy-spark-java: build",
      "type": "mvn",
      "attributes": {
        "previewUrl": ""
      }
    },
    {
      "commandLine": "clean install exec:java",
      "name": "codenvy-spark-java: execute",
      "type": "mvn",
      "attributes": {
        "previewUrl": ""
      }
    },
    {
      "commandLine": "cd /projects && git clone https://github.com/lyuboraykov/codenvy-spark-java.git",
      "name": "Initiate Project",
      "type": "custom",
      "attributes": {
        "previewUrl": ""
      }
    },
    {
      "commandLine": "cd && git clone https://github.com/lyuboraykov/codenvy-spark-java.git && cd codenvy-spark-java && mysql -u $MYSQL_USER -p$MYSQL_PASSWORD < init-db.sql",
      "name": "Initiate DB",
      "type": "custom",
      "attributes": {
        "previewUrl": ""
      }
    }
  ],
  "defaultEnv": "default",
  "name": "sparkjava-mysql",
  "environments": {
    "default": {
      "machines": {
        "db": {
          "agents": [
            "org.eclipse.che.terminal"
          ],
          "servers": {},
          "attributes": {
            "memoryLimitBytes": "1073741824"
          }
        },
        "dev-machine": {
          "agents": [
            "org.eclipse.che.terminal",
            "org.eclipse.che.ws-agent",
            "org.eclipse.che.ssh"
          ],
          "servers": {
            "4567/tcp": {
              "properties": {},
              "protocol": "http",
              "port": "4567"
            }
          },
          "attributes": {
            "memoryLimitBytes": "1073741824"
          }
        }
      },
      "recipe": {
        "contentType": "application/x-yaml",
        "type": "compose",
        "content": "services:\n  db:\n    image: codenvy/mysql\n    environment:\n      MYSQL_DATABASE: www\n      MYSQL_USER: www\n      MYSQL_PASSWORD: password\n    mem_limit: 1073741824\n  dev-machine:\n    image: codenvy/ubuntu_jdk8\n    mem_limit: 1073741824\n    depends_on:\n      - db\n"
      }
    }
  }
}